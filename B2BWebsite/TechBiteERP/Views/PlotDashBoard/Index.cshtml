<h1 class="control-label text-danger font-weight-bold ">Plot Dashboard</h1>
<div class="wrapper wrapper-content">
    <div class="row">
        <div class="ibox-content">
            <form asp-controller="PlotDashBoard" typeof="multiple"method="post">
                <div col-lg-3 class="pull-right" >
                    <button class="bg-primary" asp-controller="PlotDashBoard" asp-action="Index" asp-route-Branch="ALL"> All Branch</button>
                </div>
            </form>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            <div class="ibox float-e-margins" href="~/RealEstate/PlotRegisteration/Index">
