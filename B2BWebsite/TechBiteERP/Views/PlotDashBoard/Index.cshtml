                    @*<span class="label label-success pull-right">Monthly</span>*@
                    <a href="~/RealEstate/PlotRegisteration/PlotIndex?type=0"><h2 class="heading"> Plot</h2></a>
                </div>
                <div class="ibox-content">
                    <h1 class="no-margins">@ViewBag.TotalPlot</h1>
                    <div class="stat-percent font-bold text-success">100 % <i class="fa fa-bolt"></i></div>
                    <small><a href="~/RealEstate/PlotRegisteration/PlotIndex?type=0">Total Plots</a></small>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
